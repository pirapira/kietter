<h1>Kietter</h1>
<% if session[:notice] %>
<p>notice: <%= h(session[:notice]) %> </p>
<% end %>
<% if current_user %>
<p> <%= current_user.screen_name %> </p>
<% else %>
<p> not logged in </p>
<% end %>

<p> compare these accounts (not working for protected accounts) </p>
<%= form_tag(url_for(:action => :investigate)) do %>
<p>@<%= text_field_tag 'a' %></p>
<p>@<%= text_field_tag 'b' %></p>
<p><%= submit_tag 'compare' %>
<% end %>
